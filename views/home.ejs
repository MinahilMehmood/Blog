<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<% homePost.forEach(function(element){
    const firstKey = element.name
    const secondKey = element.post %>
    <%const maxLength = 100;
    const slicedPost =secondKey.slice(0,maxLength);%>
    <h1><%= firstKey %></h1>
    <p>
        <%= slicedPost + "..."%>
        <a href="/posts/<%= element._id %>">Read More</a>
    </p>
<% }) %>
<%- include("partials/footer") -%>